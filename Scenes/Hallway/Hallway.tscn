code = "shader_type spatial;
render_mode blend_mix, depth_draw_opaque, cull_back, diffuse_burley, specular_schlick_ggx;

uniform vec3 bottom_color : source_color = vec3(1.0, 0.6, 0.2);
uniform vec3 top_color : source_color = vec3(0.6, 0.8, 1.0);
uniform float transparency : hint_range(0.0, 1.0) = 0.55;
uniform float emission_strength : hint_range(0.0, 3.0) = 1.2;
uniform float metallic : hint_range(0.0, 1.0) = 0.1;
uniform float roughness : hint_range(0.0, 1.0) = 0.1;

void fragment() {
	vec3 gradient = mix(bottom_color, top_color, UV.y);
	ALBEDO = gradient;
	EMISSION = gradient * emission_strength;
	ALPHA = transparency;
	METALLIC = metallic;
	ROUGHNESS = roughness;
	SPECULAR = 0.8;
}
"

